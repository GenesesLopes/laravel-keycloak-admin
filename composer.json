{
    "name": "srlopes/laravel-keycloak-admin",
    "description": "This package supports Keycloak administrator client API, authenticating with username and password or using the security key (client_secret)",
    "require": {
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5@dev",
        "orchestra/testbench": "^7.0@dev"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "mnikoei",
            "email": "mohsen.nikoei@gmail.com",
            "role": "developer creator"
        },
        {
            "name": "GÃªneses Lopes",
            "email": "genesesslopes@gmail.com",
            "homepage": "https://github.com/genesesLopes",
            "role": "contributing developer"
        }
    ],
    "homepage": "https://github.com/GenesesLopes/laravel-keycloak-admin",
    "keywords": [
        "laravel",
        "keycloak"
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "KeycloakAdm\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "KeycloakAdm\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --colors='always'"
    },
    "extra": {
        "laravel": {
            "providers": [
                "KeycloakAdm\\KeycloakAdminServiceProvider"
            ]
        }
    }
}
